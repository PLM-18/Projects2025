<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">

    <persistence-unit name="objectdb-test">
        <provider>com.objectdb.jpa.PersistenceProvider</provider>
        <properties>
            <property name="jakarta.persistence.jdbc.url" value="objectdb://localhost:6136/testdb.odb"/>
            <property name="objectdb.schema" value="create"/>
        </properties>
    </persistence-unit>

</persistence>